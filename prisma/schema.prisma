datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Wine {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  country   String
  grape     String
  type      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([name], map: "unique_wine_name") // gör så att du kan testa dubblettfel
}
